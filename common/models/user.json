{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true,
    "allowEternalTokens": true
  },
  "hidden": [
    "emailVerified"
  ],
  "properties": {
    "ownerId": {
      "type": "string",
      "required": false
    },
    "credit": {
      "type": "number",
      "required": false
    },
    "create": {
      "type": "date",
      "required": false
    },
    "updated": {
      "type": "date",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "profile"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "find"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "count"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "destroyAll"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "prototype.patchAttributes"
    }
  ],
  "methods": {
    "profile": {
      "accepts": [
        {
          "id":"string"
        }
      ],
      "returns": [
        {
          "arg": "result",
          "type": "object"
        }
      ],
      "http": {
        "verb": "get"
      }
    }
  }
}

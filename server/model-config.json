{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": true
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Message": {
    "dataSource": null
  },  
  "User": {
    "dataSource": "mongoDs",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "remoting": {
        "sharedMethods": {
          "*":true,
          "profile":true,
          "login": true,
          "prototype.patchAttributes": true
        }
     }
    }
  },
  "accessToken": {
    "dataSource": "mongoDs",
    "public": false,
    "relations": {
      "User": {
        "type": "belongsTo",
        "model": "User",
        "foreignKey": "userId"
      }
    }
  },
  "member": {
    "dataSource": "member",
    "public": true
  }
}
